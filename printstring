#include "main.h"
#include <stdarg.h>

/**
 * printstring - produces output according to a format
 * @x: The argument
 *
 * Return: The number of characters that were printed
 */
int printstring(va_list x)
{
int i = 0;
char *p= va_arg(x, char *);

while (p[i] != '\0')
{
putchar(p[i]);
i++;
}
return (0);
}
